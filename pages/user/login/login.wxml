<!--pages/user/login/login.wxml-->
<view class="title">
  <text>登录</text>
</view>
<view class="content">
    <input class="input" bindinput="bindKeyusername" placeholder="登录邮箱" />
    <input class="input" bindinput="bindKeypassword" password="true" placeholder="密码" />
    <button class="btn" bindtap="bindload">登陆</button>
</view>